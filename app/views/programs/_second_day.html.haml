          %li.b-programm__list-item.js-b-programm__content
            = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('shapiro-masterclass'), :as => :speech
    .b-programm__row
      .b-programm__head-title.clearfix
        .l-page_float_left.l-page_width_80
