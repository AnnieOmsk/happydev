-# = javascript_include_tag 'blocks/b-programm/b-programm-likes_getter'

// Second_Day ////////////////
%section#programm.b-programm.l-page_gap-top-out_30
  #second_day
  .l-page_align_center.b-title__line
    .b-title__inner
      %h2.b-title
        Программа 30 сентября

  %table.b-programm__table{:style => 'margin-top: 45px;'}
    %tbody.b-programm__body
      %tr.b-programm__row.b-programm__row_state_filled
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            9:00
        %td.b-programm__column
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('byndyu-masterclass'), :as => :speech
        %td.b-programm__column
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('ivanischev-planning'), :as => :speech
        %td.b-programm__column{:colspan => '2'}
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('shapiro-masterclass'), :as => :speech
      %tr.b-programm__row
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            11:00
        %td.b-programm__column
        %td.b-programm__column
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('ivanischev-idea'), :as => :speech
        %td.b-programm__column
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('podorozhny-masterclass'), :as => :speech
        %td.b-programm__column
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('nepomnyashikh-masterclass'), :as => :speech
      %tr.b-programm__row.b-programm__row_state_filled
        %td.b-programm__column.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            13:00
        %td.b-programm__column{:colspan => "4"}
          %ul.b-icon
            %li.b-icon__item
              -# %span.b-icon__icon.b-icon__icon_lunch
              = wicked_pdf_image_tag 'pdf_program_icons/icons_02.svg'
          %p.b-programm__text
            Обед
      %tr.b-programm__row
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            14:00
        %td.b-programm__column.b-program__column_type_large{:colspan => "4"}
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('bektimirov'), :as => :speech
      %tr.b-programm__row.b-programm__row_state_filled
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            14:35
        %td.b-programm__column.b-program__column_type_large{:colspan => "4"}
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('mikodin'), :as => :speech
      %tr.b-programm__row
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            15:00
        %td.b-programm__column.b-program__column_type_large{:colspan => "4"}
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('belokrys'), :as => :speech
      %tr.b-programm__row.b-programm__row_state_filled
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            16:10
        %td.b-programm__column.b-program__column_type_large{:colspan => "4"}
          = render :partial => "speeches/small_preview", :object => @speeches.find_by_permalink('shibaev'), :as => :speech
      %tr.b-programm__row
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            17:00
        %td.b-programm__column
          .b-program__text
            %a.b-programm__theme-link{:href => ''} Круглый стол по ИТ-образованию
        %td.b-programm__column
        %td.b-programm__column{:colspan => "2"}
          .b-program__text
            %a.b-programm__theme-link{:href => ''} World cafe, open space и другие креативные тусовки
      %tr.b-programm__row.b-programm__row_state_filled
        %td.b-programm__column.b-programm__column_width_40
          %p.b-programm__text
            18:00
        %td.b-programm__column{:colspan => "4"}
          %ul.b-icon
            %li.b-icon__item
              -# %span.b-icon__icon.b-icon__icon_end
              = wicked_pdf_image_tag 'pdf_program_icons/icons_06.svg'
          %p.b-programm__text
            Закрытие конференции
